{
    "compilerOptions": {
        "removeComments": false,
        "preserveConstEnums": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "declaration": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "module": "commonjs",
        "noEmitHelpers": false,
        "target": "ES5",
        "outDir": "./",
        "lib": [
            "es5"
        ]
    },
    "files": [
        "src/index.ts"
    ]
}