{
    "name": "xstream-pipe",
    "version": "0.0.0",
    "description": "Pipeable operators for xstream's core operators.",
    "main": "index.js",
    "typings": "index.t.ts",
    "scripts": {
        "commit": "git-cz",
        "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --release-count 0 --preset angular",
        "build": "tsc",
        "lint": "tslint -c tslint.json src/*.ts",
        "prepublish": "npm run build",
        "version": "npm run lint && npm run changelog && git add -A"
    },
    "repository": "https://github.com/anilanar/xstream-pipe",
    "bugs": {
        "url": "https://github.com/anilanar/xstream-pipe"
    },
    "homepage": "https://github.com/anilanar/xstream-pipe",
    "author": "Anil Anar",
    "license": "MIT",
    "dependencies": {
        "xstream": "^0.0.0"
    },
    "devDependencies": {
        "commitizen": "^2.9.6",
        "conventional-changelog-cli": "^1.3.22",
        "cz-conventional-changelog": "^2.1.0",
        "tslint": "^5.10.0",
        "typescript": "^2.8.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
